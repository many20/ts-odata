{
  "version": "0.1.0",
  "name": "ts-odata",
  "description": "A typescript library to help generate odata queries",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrekkingForCharity/ts-odata.git"
  },
  "bugs": "https://github.com/TrekkingForCharity/ts-odata/issues",
  "homepage": "https://github.com/TrekkingForCharity/ts-odata",
  "author": "Trekking for Charity",
  "contributors": [
    "Andrew Davis"
  ],
  "keywords": [
    "jodata",
    "odata"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "npm run clean && npm run lint && echo Using TypeScript && tsc --version && tsc --pretty",
    "test": "npm run build && mocha --compilers ts:ts-node/register --recursive test/**/*-spec.ts",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "postinstall": "tsc"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^3.0.0",
    "@types/mocha": "^2.0.0",
    "@types/node": "6.0.31",
    "autodts": "0.0.6",
    "chai": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.0.0",
    "rimraf": "^2.0.0",
    "semantic-release": "^6.3.2",
    "ts-node": "^1.0.0",
    "tslint": "^4.0.0",
    "typescript": "~2.3.0"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
